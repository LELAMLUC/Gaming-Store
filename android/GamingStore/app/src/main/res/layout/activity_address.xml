<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_all">


    <ImageView
        android:id="@+id/imageView55"
        android:layout_width="340dp"
        android:layout_height="468dp"
        android:layout_marginTop="28dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView52"
        app:srcCompat="@drawable/khung_createacc" />


    <ImageButton
        android:id="@+id/btnRegister4"
        android:layout_width="285dp"
        android:layout_height="49dp"
        android:layout_marginBottom="24dp"
        android:background="@drawable/cyber_ripple"
        android:contentDescription="@null"
        android:foreground="@drawable/cyber_ripple"
        android:scaleType="fitXY"
        android:src="@drawable/bg_button"
        app:layout_constraintBottom_toBottomOf="@+id/imageView55"
        app:layout_constraintEnd_toEndOf="@+id/imageView55"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="@+id/imageView55" />

    <ImageView
        android:id="@+id/imageView52"
        android:layout_width="393dp"
        android:layout_height="1dp"
        android:layout_marginTop="26dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView14"
        app:srcCompat="@drawable/vien_xam" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="26dp"
        android:text="Change Address"
        android:textColor="#FFFFFF"
        android:textSize="32sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.13"
        app:layout_constraintStart_toEndOf="@+id/imageView48"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView48"
        android:layout_width="24dp"
        android:layout_height="25dp"
        android:layout_marginTop="36dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.074"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/icon_muitentrai" />

    <ImageView
        android:id="@+id/imageView59"
        android:layout_width="285dp"
        android:layout_height="49dp"
        android:layout_marginTop="14dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView55"
        app:layout_constraintStart_toStartOf="@+id/imageView55"
        app:layout_constraintTop_toTopOf="@+id/imageView55"
        app:srcCompat="@drawable/khung_email" />

    <EditText
        android:id="@+id/edtName"
        android:layout_width="260dp"
        android:layout_height="48dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:hint="Full Name"
        android:inputType="textEmailAddress"
        android:textColor="#FFFFFF"
        android:textColorHint="#69707E"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView59"
        app:layout_constraintEnd_toEndOf="@+id/imageView59"
        app:layout_constraintStart_toStartOf="@+id/imageView59"
        app:layout_constraintTop_toTopOf="@+id/imageView59" />

    <ImageView
        android:id="@+id/imageView60"
        android:layout_width="285dp"
        android:layout_height="49dp"
        android:layout_marginTop="14dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView55"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="@+id/imageView55"
        app:layout_constraintTop_toBottomOf="@+id/imageView59"
        app:srcCompat="@drawable/khung_email" />

    <EditText
        android:id="@+id/edtPhone"
        android:layout_width="260dp"
        android:layout_height="48dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:hint="Phone Number"
        android:inputType="phone"
        android:textColor="#FFFFFF"
        android:textColorHint="#69707E"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView60"
        app:layout_constraintEnd_toEndOf="@+id/imageView60"
        app:layout_constraintHorizontal_bias="0.52"
        app:layout_constraintStart_toStartOf="@+id/imageView60"
        app:layout_constraintTop_toTopOf="@+id/imageView60"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/imageView61"
        android:layout_width="285dp"
        android:layout_height="49dp"
        android:layout_marginTop="14dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView55"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="@+id/imageView55"
        app:layout_constraintTop_toBottomOf="@+id/imageView60"
        app:srcCompat="@drawable/khung_email" />

    <EditText
        android:id="@+id/edtCity"
        android:layout_width="260dp"
        android:layout_height="48dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:hint="Province / City"
        android:inputType="text"
        android:textColor="#FFFFFF"
        android:textColorHint="#69707E"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView61"
        app:layout_constraintEnd_toEndOf="@+id/imageView61"
        app:layout_constraintHorizontal_bias="0.52"
        app:layout_constraintStart_toStartOf="@+id/imageView61"
        app:layout_constraintTop_toTopOf="@+id/imageView61"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/imageView62"
        android:layout_width="285dp"
        android:layout_height="49dp"
        android:layout_marginTop="14dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView55"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="@+id/imageView55"
        app:layout_constraintTop_toBottomOf="@+id/imageView61"
        app:srcCompat="@drawable/khung_email" />

    <EditText
        android:id="@+id/edtDistrict"
        android:layout_width="260dp"
        android:layout_height="48dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:hint="District"
        android:inputType="text"
        android:textColor="#FFFFFF"
        android:textColorHint="#69707E"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView62"
        app:layout_constraintEnd_toEndOf="@+id/imageView62"
        app:layout_constraintHorizontal_bias="0.52"
        app:layout_constraintStart_toStartOf="@+id/imageView62"
        app:layout_constraintTop_toTopOf="@+id/imageView62"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/imageView63"
        android:layout_width="285dp"
        android:layout_height="49dp"
        android:layout_marginTop="14dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView55"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="@+id/imageView55"
        app:layout_constraintTop_toBottomOf="@+id/imageView62"
        app:srcCompat="@drawable/khung_email" />

    <EditText
        android:id="@+id/edtWard"
        android:layout_width="260dp"
        android:layout_height="48dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:hint="Ward / Commune"
        android:inputType="text"
        android:textColor="#FFFFFF"
        android:textColorHint="#69707E"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView63"
        app:layout_constraintEnd_toEndOf="@+id/imageView63"
        app:layout_constraintHorizontal_bias="0.52"
        app:layout_constraintStart_toStartOf="@+id/imageView63"
        app:layout_constraintTop_toTopOf="@+id/imageView63"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/imageView64"
        android:layout_width="285dp"
        android:layout_height="49dp"
        android:layout_marginTop="14dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView55"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="@+id/imageView55"
        app:layout_constraintTop_toBottomOf="@+id/imageView63"
        app:srcCompat="@drawable/khung_email" />

    <EditText
        android:id="@+id/edtAddress"
        android:layout_width="260dp"
        android:layout_height="48dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:hint="Detailed Address"
        android:inputType="text"
        android:textColor="#FFFFFF"
        android:textColorHint="#69707E"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView64"
        app:layout_constraintEnd_toEndOf="@+id/imageView64"
        app:layout_constraintHorizontal_bias="0.52"
        app:layout_constraintStart_toStartOf="@+id/imageView64"
        app:layout_constraintTop_toTopOf="@+id/imageView64"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Complete"
        android:textColor="#0ABDE3"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/btnRegister4"
        app:layout_constraintEnd_toEndOf="@+id/btnRegister4"
        app:layout_constraintStart_toStartOf="@+id/btnRegister4"
        app:layout_constraintTop_toTopOf="@+id/btnRegister4" />
</androidx.constraintlayout.widget.ConstraintLayout>